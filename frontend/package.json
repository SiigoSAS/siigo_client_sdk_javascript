{
  "name": "frontend",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json --host 0.0.0.0 --disable-host-check --poll 100",
    "serve": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run frontend:serve-ssr",
    "serve:ssr": "node dist/frontend/server/main.js",
    "build:prod": "ng build --configuration production --output-path=dist",
    "build:ssr": "ng build --configuration production && ng run frontend:server:production",
    "build:output": "npm run build:ssr --output-path=dist",
    "prerender": "ng run frontend:prerender",
    "dev-server": "concurrently \"nodemon ../api/server.js --watch api\" \"npm run serve\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.12",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "~12.2.12",
    "@angular/compiler": "~12.2.12",
    "@angular/core": "~12.2.12",
    "@angular/forms": "~12.2.12",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "~12.2.12",
    "@angular/platform-browser-dynamic": "~12.2.12",
    "@angular/platform-server": "~12.2.12",
    "@angular/router": "~12.2.12",
    "@nguniversal/express-engine": "^12.1.3",
    "@swimlane/ngx-datatable": "^20.0.0",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.7.1",
    "express": "^4.15.2",
    "font-awesome": "^4.7.0",
    "materialize-css": "^1.0.0-rc.2",
    "ng-http-loader": "^11.0.1",
    "ngx-bootstrap": "^7.1.0",
    "ngx-toastr": "^14.0.0",
    "rxjs": "^6.6.7",
    "sweetalert2": "^11.2.1",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.12",
    "@angular/cli": "^12.2.12",
    "@angular/compiler-cli": "~12.2.12",
    "@angular/language-service": "^12.2.12",
    "@nguniversal/builders": "^13.1.1",
    "@types/express": "^4.17.9",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^16.11.6",
    "codelyzer": "^6.0.2",
    "concurrently": "6.2",
    "jasmine-core": "^3.7.1",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.6.0",
    "protractor": "~7.0.0",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typescript": ">=4.2.3 <4.4"
  }
}
