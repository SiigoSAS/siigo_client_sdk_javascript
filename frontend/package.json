{
  "name": "frontend",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json --host 0.0.0.0 --disable-host-check --poll 100",
    "serve": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run frontend:serve-ssr",
    "serve:ssr": "node dist/frontend/server/main.js",
    "build:prod": "ng build --configuration production --output-path=dist",
    "build:ssr": "ng build --configuration production && ng run frontend:server:production",
    "build:output": "npm run build:ssr --output-path=dist",
    "prerender": "ng run frontend:prerender",
    "dev-server": "concurrently \"nodemon ../api/server.js --watch api\" \"npm run serve\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.7",
    "@angular/cdk": "^16.2.6",
    "@angular/common": "^16.2.7",
    "@angular/compiler": "^16.2.7",
    "@angular/core": "^16.2.7",
    "@angular/forms": "^16.2.7",
    "@angular/material": "^16.2.6",
    "@angular/platform-browser": "^16.2.7",
    "@angular/platform-browser-dynamic": "^16.2.7",
    "@angular/platform-server": "^16.2.7",
    "@angular/router": "^16.2.7",
    "@nguniversal/express-engine": "^16.2.0",
    "@popperjs/core": "^2.11.8",
    "@swimlane/ngx-datatable": "^20.1.0",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.1",
    "express": "^4.18.2",
    "font-awesome": "^4.7.0",
    "ng-http-loader": "^14.0.0",
    "ngx-bootstrap": "^11.0.2",
    "ngx-toastr": "^17.0.2",
    "rxjs": "^7.8.1",
    "sweetalert2": "^11.7.31",
    "tslib": "^2.6.2",
    "zone.js": "0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.4",
    "@angular/cli": "^16.2.4",
    "@angular/compiler-cli": "^16.2.7",
    "@angular/language-service": "^16.2.7",
    "@nguniversal/builders": "^16.2.0",
    "@types/express": "^4.17.18",
    "@types/jasmine": "~4.3.6",
    "@types/jasminewd2": "~2.0.11",
    "@types/node": "^20.8.2",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "concurrently": "~8.2.1",
    "eslint": "^8.50.0",
    "jasmine-core": "^5.1.1",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "ts-node": "^10.9.1",
    "typescript": "4.9.5"
  }
}
